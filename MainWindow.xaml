<Window x:Class="DSAE_HonoursGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DSAE_HonoursGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="ReportBox" HorizontalAlignment="Left" Height="330" Margin="10,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="772"/>
        <Button x:Name="StartScrap" Content="Begin Scrapping" HorizontalAlignment="Left" Margin="27,376,0,0" VerticalAlignment="Top" Width="129" Click="Button_Click"/>
        <Button x:Name="PauseScrap" Content="Pause" HorizontalAlignment="Left" VerticalAlignment="Top" Width="102" Margin="187,376,0,0"/>
        <Button x:Name="StopScrap" Content="Stop" HorizontalAlignment="Left" Margin="341,376,0,0" VerticalAlignment="Top" Width="75" Click="StopScrap_Click"/>

    </Grid>
</Window>
